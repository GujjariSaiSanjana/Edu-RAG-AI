{
  
  "compilerOptions": {
  "target": "ES2022",
  "module": "ES2022",
  "moduleResolution": "NodeNext",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "outDir": "dist",
  "rootDir": "src",
  "resolveJsonModule": true,
  "allowSyntheticDefaultImports": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
